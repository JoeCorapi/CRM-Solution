<view>
  <fetchxml>
    <fetch version="1.0" output-format="xml-platform" mapping="logical" distinct="false">
      <entity name="product">
        <attribute name="name" />
      </entity>
    </fetch>
  </fetchxml>
  <layoutxml>
    <grid name="resultset" object="1024" jump="name" select="1" preview="1" icon="1">
      <row name="result" id="productid">
        <cell name="name" width="150" />
      </row>
    </grid>
  </layoutxml>
</view>